{
  "name": "express-user-api",
  "version": "1.0.0",
  "description": "Express API integration with @smendivil/user module",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
